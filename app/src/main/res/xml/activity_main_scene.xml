<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet></KeyFrameSet>
        <OnSwipe motion:touchAnchorId="@+id/button" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView7"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintVertical_bias="0.959"
            motion:layout_constraintHorizontal_bias="0.501"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintStart_toStartOf="@+id/view"
            android:alpha="0" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintVertical_bias="0.959"
            motion:layout_constraintHorizontal_bias="0.504"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintStart_toStartOf="@+id/view" />
        <Constraint
            android:id="@+id/imageView8"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintVertical_bias="0.959"
            motion:layout_constraintHorizontal_bias="0.504"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintStart_toStartOf="@+id/view"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView9"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintVertical_bias="0.959"
            motion:layout_constraintHorizontal_bias="0.504"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintStart_toStartOf="@+id/view"
            android:alpha="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.045"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/view"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.047"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:id="@+id/imageView7" />
        <Constraint
            motion:layout_constraintVertical_bias="0.049"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/view"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:id="@+id/imageView8" />
        <Constraint
            motion:layout_constraintVertical_bias="0.045"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/view"
            motion:layout_constraintTop_toTopOf="@+id/view"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintHorizontal_bias="0.946"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9" />
    </ConstraintSet>
</MotionScene>